{
{{#if title}}
	title: '{{{title}}}',
{{/if}}
{{#if description}}
	description: `{{{escapeDescription description}}}`,
{{/if}}
	type: 'Enum',
{{#if isReadOnly}}
	isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
	isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
	isNullable: {{{isNullable}}},
{{/if}}
	items: [
{{#each enum}}
		{
			value: {{{../name}}}.{{{name}}},
			varname: "{{{name}}}",
			title: "{{{title}}}",
			description: "{{{description}}}",
		},
{{/each}}
	],
}
