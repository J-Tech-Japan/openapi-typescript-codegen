{
{{#if title}}
	title: '{{{title}}}',
{{/if}}
{{#if description}}
	description: `{{{escapeDescription description}}}`,
{{/if}}
	type: 'Enum',
{{#if isReadOnly}}
	isReadOnly: {{{isReadOnly}}},
{{/if}}
{{#if isRequired}}
	isRequired: {{{isRequired}}},
{{/if}}
{{#if isNullable}}
	isNullable: {{{isNullable}}},
{{/if}}
	items: [
{{#each enum}}
		{
{{#if @root.useUnionTypes}}
			value: {{{value}}},
{{else}}
			value: {{{../name}}}.{{{name}}},
{{/if}}
			varname: '{{{name}}}',
			enumValue: {{{enumValue}}},
			title: '{{{title}}}',
			description: '{{{description}}}',
		},
{{/each}}
	],
}
